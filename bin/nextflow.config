profiles {
    standard {
        executor = 'local'
        docker.enabled = true

        process {
            withName: 'blast' {
                container = 'hadrieng/blast:2.7.1'
            }
        }
        process {
            withName: 'kraken' {
                container = 'hadrieng/kraken:1.1'
            }
        }
    }
}